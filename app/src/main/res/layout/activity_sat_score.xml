<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.a20190602_kwawannan_nycschools.view.SatScoreActivity">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="191dp"
        android:orientation="vertical"
        android:layout_gravity="center"
        android:layout_marginTop="200dp">

        <TextView
            android:id="@+id/high_school_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            android:text="HighSchool" />

        <TextView
            android:id="@+id/sat_Score"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            android:text="mathSatScore" />

        <TextView
            android:id="@+id/reading_sat_score"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            android:text="readingSatScore" />

        <TextView
            android:id="@+id/writing_sat_score"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            android:text="writingSatScore" />

    </LinearLayout>


</LinearLayout>